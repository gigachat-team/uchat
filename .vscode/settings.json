{
    "clangd.fallbackFlags": [
        "-I${workspaceFolder}/server",
        "-I${workspaceFolder}/server/inc",
        "-I${workspaceFolder}/client",
        "-I${workspaceFolder}/client/inc",
        "-I${workspaceFolder}/utils",
        "-I${workspaceFolder}/utils/inc",
        "-I${workspaceFolder}/resources/libraries/sqlite3",
        "-I/usr/include/gtk-3.0",
        "-I/usr/include/glib-2.0",
        "-I/usr/lib/x86_64-linux-gnu/glib-2.0/include",
        "-I/usr/include/pango-1.0",
        "-I/usr/include/harfbuzz",
        "-I/usr/include/cairo",
        "-I/usr/include/gdk-pixbuf-2.0",
        "-I/usr/include/atk-1.0"
    ],

    "files.associations": {
        "*.tcc": "c",
        "optional": "c",
        "memory": "c",
        "numeric": "c",
        "cmath": "c",
        "typeinfo": "c",
        "glib.h": "c",
        "gtk.h": "c",
        "gsignal.h": "c",
        "client.h": "c"
    },
    "C_Cpp.vcFormat.newLine.beforeElse": true,
    "C_Cpp.vcFormat.space.betweenEmptyBraces": false,
    "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,
    "notebook.formatOnSave.enabled": false,
    "editor.formatOnSave": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine"
}
